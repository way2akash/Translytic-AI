{
  "name": "content",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "rewire": "^6.0.0",
    "styled-components": "^6.1.13",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "node scripts/build.js && cp build/index.js ../extension",
    "watch-build": "nodemon --exec 'npm run build' --watch src --watch public --ext js,jsx,ts,tsx,css,json && cp build/index.js ../extension",
    "build-win": "node scripts/build.js && xcopy /Y build\\content.js ..\\extension",
    "watch-build-win": "nodemon --exec \"npm run build\" --watch src --watch public --ext js,jsx,ts,tsx,css,json && xcopy /Y build\\content.js ..\\extension",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
